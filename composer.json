{
    "name": "asseco-voice/laravel-open-api",
    "description": "Laravel OpenAPI generator",
    "keywords": [
        "php",
        "laravel",
        "openapi"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "laravel/framework": "^8.0",
        "symfony/yaml": "^5.1",
        "mpociot/reflection-docblock": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "^v6.22.0",
        "spatie/phpunit-watcher": "^1.23.2"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Asseco\\OpenApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\OpenApi\\OpenApiServiceProvider"
            ]
        }
    }
}
